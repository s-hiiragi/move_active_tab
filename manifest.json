{
	"manifest_version": 2,
	"name": "move_active_tab",
	"version": "1.0.0.0",
	
	"description": "Move active tab to not focused window",
	
	"background": {
		"scripts": ["src/background.js"],
		"persistent": true
	},
	
	"commands": {
		"move_tab": {
			"suggested_key": {
				"default": "Ctrl+M",
				"mac": "Command+M"
			},
			"description": "Move active tab to last focused window"
		}
	},
	
	"permissions": [
		"tabs"
	]
}
